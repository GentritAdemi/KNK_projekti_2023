<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: darkblue;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projektiknk.Controller.LoginController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="200.0" layoutY="-4.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="408.0" fitWidth="164.0">
         <image>
            <Image url="@NBA.jpeg" />
         </image>
      </ImageView>
      <TextField fx:id="username" layoutX="299.0" layoutY="127.0" promptText="Enter your username:" style="-fx-border-style: solid;" />
      <PasswordField fx:id="password" layoutX="298.0" layoutY="196.0" prefHeight="28.0" prefWidth="151.0" promptText="Enter your password:" style="-fx-border-style: solid;" />
      <Button fx:id="Login" layoutX="337.0" layoutY="289.0" mnemonicParsing="false" onAction="#loginClick" prefHeight="28.0" prefWidth="74.0" text="Login" />
      <Button fx:id="SignUp" layoutX="498.0" layoutY="288.0" mnemonicParsing="false" onAction="#gotoSignUp" prefHeight="28.0" prefWidth="85.0" text="SignUp"  />
      <ToggleButton fx:id="Translate" layoutX="186.0" layoutY="291.0" mnemonicParsing="false" onAction="#translateClick" prefHeight="25.0" prefWidth="85.0" text="Translate"  />
   </children>
</AnchorPane>
